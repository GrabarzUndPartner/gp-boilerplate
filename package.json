{
  "name": "gp-boilerplate",
  "version": "5.0.0",
  "description": "Grabarz & Partner - Frontend Boilerplate",
  "main": "none",
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/GrabarzUndPartner/gp-boilerplate/issues"
  },
  "homepage": "https://github.com/GrabarzUndPartner/gp-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrabarzUndPartner/gp-boilerplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@grabarzundpartner.de"
    },
    {
      "name": "Thorn Walli",
      "email": "thorn.walli@grabarzundpartner.de"
    }
  ],
  "license": "GNU GPLv3",
  "author": "Grabarz & Partner Werbeagentur GmbH",
  "scripts": {
    "test": "npm run lint && npm run build",
    "prod": "cross-env NODE_ENV=production node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=./env/server",
    "build": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp build --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "dev": "cross-env NODE_ENV=development node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=./env/server",
    "server": "cross-env NODE_ENV=production node node_modules/gulp/bin/gulp default --serverConfig=/env/config/hapi/config --server=./env/server",
    "debug": "npm run build",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:es": "eslint --fix --ext .js --ext .json ./",
    "lint:css": "stylelint \"src/**/*.pcss\"",
    "task-postcss": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp postcss --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-handlebars": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp handlebars --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-generates": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp generates --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-webpack-app": "cross-env NODE_ENV=production node node_modules/gulp/bin/gulp webpack:app --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-svgo": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp svgo --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-svg-symbols": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp svg-symbols --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "deploy-now": "npm run build && now ./build -A ../now.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "acorn": "^7.1.0",
    "ampersand-collection": "^2.0.2",
    "ampersand-model": "^8.0.1",
    "ampersand-view": "^10.0.3",
    "ansi-colors": "^4.1.1",
    "assemble": "^0.24.3",
    "assemble-handlebars-helpers": "^0.1.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "cheerio": "^0.22.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "cssmin": "^0.4.3",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "eslint-config-stylelint": "^11.1.0",
    "fg-loadcss": "^2.1.0",
    "fontfaceobserver": "^2.1.0",
    "fs-extra": "^8.1.0",
    "gulp-changed": "^4.0.2",
    "gulp-extname": "^0.2.2",
    "gulp-nodemon": "^2.4.2",
    "gulp-notify": "^3.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-svg-symbols": "^3.2.3",
    "gulp-svgo": "^2.1.1",
    "h2o2": "^8.2.0",
    "handlebars": "^4.7.2",
    "handlebars-layouts": "^3.1.4",
    "handlebars-loader": "^1.7.1",
    "handlebars-utils": "^1.0.6",
    "hapi": "^18.1.0",
    "hapi-auth-jwt2": "^8.8.1",
    "highlight.js": "^9.18.0",
    "inert": "^5.1.3",
    "jquery": "^3.4.1",
    "js-prettify": "^1.4.0",
    "lazysizes": "^5.2.0",
    "object-fit-images": "^3.2.4",
    "optimize-js-plugin": "0.0.4",
    "postcss-assets": "^5.0.0",
    "postcss-bootstrap-4-grid": "^1.0.3",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-calc": "^7.0.1",
    "postcss-clearfix": "^2.0.1",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-momentum-scrolling": "^2.2.9",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "precss": "^4.0.0",
    "regenerator-runtime": "^0.13.3",
    "script-loader": "^0.7.2",
    "snyk": "^1.290.1",
    "string-replace-loader": "^2.2.0",
    "template-helpers": "^1.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uniq": "^1.0.1",
    "upath": "^1.2.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-stream": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.3.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-mocha": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-livereload": "^4.0.2",
    "run-sequence": "^2.2.1",
    "standard": "^14.3.1",
    "stylelint": "^13.1.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^19.0.0",
    "sw-precache-webpack-plugin": "^1.0.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-pwa-manifest": "^4.1.1"
  },
  "snyk": true
}
