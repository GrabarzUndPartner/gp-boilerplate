{
  "name": "gp-boilerplate",
  "version": "4.2.7",
  "description": "Grabarz & Partner - Frontend Boilerplate",
  "main": "none",
  "bugs": {
    "url": "https://github.com/GrabarzUndPartner/gp-boilerplate/issues"
  },
  "homepage": "https://github.com/GrabarzUndPartner/gp-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrabarzUndPartner/gp-boilerplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@grabarzundpartner.de"
    },
    {
      "name": "Thorn Walli",
      "email": "thorn.walli@grabarzundpartner.de"
    }
  ],
  "license": "MIT",
  "private": true,
  "author": "Grabarz & Partner Werbeagentur GmbH",
  "scripts": {
    "test": "npm run lint && npm run build",
    "prod": "cross-env NODE_ENV=production node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=./env/server",
    "build": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp build --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "dev": "cross-env NODE_ENV=development node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=./env/server",
    "server": "cross-env NODE_ENV=production node node_modules/gulp/bin/gulp default --serverConfig=/env/config/hapi/config --server=./env/server",
    "debug": "npm run build",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:es": "eslint --fix --ext .js --ext .json ./",
    "lint:css": "stylelint \"src/**/*.pcss\"",
    "task-postcss": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp postcss --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-handlebars": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp handlebars --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-generates": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp generates --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-webpack-app": "cross-env NODE_ENV=production node node_modules/gulp/bin/gulp webpack:app --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-svgo": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp svgo --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "task-svg-symbols": "cross-env NODE_ENV=build node node_modules/gulp/bin/gulp svg-symbols --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "deploy-now": "npm run build && now ./build -A ../now.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@hapi/hapi": "20.1.0",
    "acorn": "8.0.5",
